server {
	listen 8001;
	server_name localhost;

	location / {
        proxy_set_header Host $host;
		proxy_pass http://django_channel_1:8000;
	}
}

server {
	listen 8002;
	server_name localhost;

	location / {
		proxy_set_header Host $host;
		proxy_pass http://django_channel_2:8000;
	}

}

upstream devs {
	server localhost:8001;
	server localhost:8002;
}

server {
	listen 9400;
	server_name localhost;

	location / {
		proxy_pass http://devs;
	}
	location /static/ {
		alias /var/www/static/;
	}
}
